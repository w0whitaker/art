---
layout: "layouts/base.njk"
---
<header>
  <h1>{{ metadata.title }}</h1>
  <a href="/">{{ metadata.displayName }}</a>
  <nav>
    <ul>
      {%- for entry in collections.all | eleventyNavigation -%}
        <li>
          <a href="{{ entry.url }}">{{ entry.title }}</a>
        </li>
      {%- endfor -%}
    </ul>
  </nav>
</header>
{{ content | safe }}